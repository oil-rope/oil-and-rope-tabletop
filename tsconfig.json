{
  "compilerOptions": {
    "target": "ES3",
    "lib": ["DOM", "ES2021"],
    "jsx": "react",
    "module": "ESNext",
    "rootDir": ".",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "outDir": "./vendor/",
    "newLine": "lf",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./src/*"],
      "@Components/*": ["./src/components/*"],
    }
  }
}
